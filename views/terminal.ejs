    <script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.12"></script>
    <script>
        var typed = new Typed('.endCmd', {strings: ['$desktop spg conclusion<br/><br/>Thanks for using Simple Password Generator!<br />Github: Soon<br />We do not track you or store generated passwords in anyway, it is just a simple JavaScript script. Your data is 100% secure &amp; this project is open source.']})
    </script>
    <script>
        function gen(length = 30) {
        var buf = new Uint8Array(length);window.crypto.getRandomValues(buf);
        var value = btoa(String.fromCharCode.apply(null, buf)).replace(/\//g,"_").replace(/\+/g,"").replace(/\=/g,"").substring(0,length)
        document.getElementById('pw').innerHTML = value
        }
        gen()
    </script>
